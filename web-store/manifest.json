  "oauth2": {
    "client_id": "313416032683-3ob52hbv8u7gl4hi8l3sa6eh5ru4rb6u.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/chromewebstore"]
  }
}
